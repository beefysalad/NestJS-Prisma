// This is your Prisma schema file,
// learn more about it in the docs: https://pris.ly/d/prisma-schema

generator client {
  provider = "prisma-client-js"
}

datasource db {
  provider = "postgresql"
  url      = env("DATABASE_URL")
}

model User{
  id      Int     @id @default(autoincrement())
  email   String  @unique
  name    String?
  age     Int
  role    Role
}

model Organizer {
  id      Int @id @default(autoincrement())
  name    String
  type    Type

}

enum Type {
  DEPARTMENT
  ORGANIZATION
  OTHERS
}

enum Role{
  STAFF
  ADMIN
}